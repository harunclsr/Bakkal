<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#AF0303"
    tools:context=".AnaEkran">

    <ImageButton
        android:id="@+id/arama"
        android:layout_width="85dp"
        android:layout_height="84dp"
        android:background="#FAF6F6"
        android:tint="#AF0303"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@android:drawable/ic_menu_search" />

    <ImageButton
        android:id="@+id/icecekler"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.049"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.398"
        app:srcCompat="@drawable/i_ecekler" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="İçecekler"
        app:layout_constraintBottom_toTopOf="@+id/icecekler"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.079"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <ImageButton
        android:id="@+id/manav"
        android:layout_width="116dp"
        android:layout_height="110dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.083"
        app:layout_constraintStart_toEndOf="@+id/icecekler"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.396"
        app:srcCompat="@drawable/manav" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Manav"
        app:layout_constraintBottom_toTopOf="@+id/manav"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.242"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <ImageButton
        android:id="@+id/k.bakim"
        android:layout_width="130dp"
        android:layout_height="110dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.644"
        app:layout_constraintStart_toEndOf="@+id/manav"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.399"
        app:srcCompat="@drawable/kiisel_bakm__r_nleri" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="154dp"
        android:layout_height="19dp"
        android:text="Kişisel Bakım Ürünleri"
        app:layout_constraintBottom_toTopOf="@+id/k.bakim"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.983"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.991" />

    <ImageButton
        android:id="@+id/T.urunleri"
        android:layout_width="114dp"
        android:layout_height="101dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.056"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/icecekler"
        app:layout_constraintVertical_bias="0.189"
        app:srcCompat="@drawable/temizlik__r_nleri" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="113dp"
        android:layout_height="19dp"
        android:text="Temizlik Ürünleri"
        app:layout_constraintBottom_toTopOf="@+id/T.urunleri"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.043"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/icecekler"
        app:layout_constraintVertical_bias="1.0" />

    <ImageButton
        android:id="@+id/et"
        android:layout_width="130dp"
        android:layout_height="97dp"
        android:layout_marginTop="48dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.093"
        app:layout_constraintStart_toEndOf="@+id/T.urunleri"
        app:layout_constraintTop_toBottomOf="@+id/k.bakim"
        app:layout_constraintVertical_bias="0.008"
        app:srcCompat="@drawable/et" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="45dp"
        android:layout_height="24dp"
        android:text="Et"
        app:layout_constraintBottom_toTopOf="@+id/et"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.183"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/manav"
        app:layout_constraintVertical_bias="1.0" />

    <ImageButton
        android:id="@+id/ekmek"
        android:layout_width="116dp"
        android:layout_height="99dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toEndOf="@+id/et"
        app:layout_constraintTop_toBottomOf="@+id/k.bakim"
        app:layout_constraintVertical_bias="0.175"
        app:srcCompat="@drawable/ekmek" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="74dp"
        android:layout_height="21dp"
        android:text="Ekmek"
        app:layout_constraintBottom_toTopOf="@+id/ekmek"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.606"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/k.bakim"
        app:layout_constraintVertical_bias="0.962" />

    <ImageButton
        android:id="@+id/atistirmalik"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.06"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/T.urunleri"
        app:layout_constraintVertical_bias="0.359"
        app:srcCompat="@drawable/attrmalk" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="92dp"
        android:layout_height="21dp"
        android:text="Atıştırmalıklar"
        app:layout_constraintBottom_toTopOf="@+id/atistirmalik"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.079"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/T.urunleri"
        app:layout_constraintVertical_bias="1.0" />

    <ImageButton
        android:id="@+id/kahvaltilik"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toEndOf="@+id/atistirmalik"
        app:layout_constraintTop_toBottomOf="@+id/et"
        app:layout_constraintVertical_bias="0.381"
        app:srcCompat="@drawable/kahvaltlk__r_nler" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="124dp"
        android:layout_height="26dp"
        android:text="Kahvaltılık Ürünler"
        app:layout_constraintBottom_toTopOf="@+id/kahvaltilik"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.164"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/et"
        app:layout_constraintVertical_bias="1.0" />

    <ImageButton
        android:id="@+id/temel.gida"
        android:layout_width="133dp"
        android:layout_height="105dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.304"
        app:layout_constraintStart_toEndOf="@+id/kahvaltilik"
        app:layout_constraintTop_toBottomOf="@+id/ekmek"
        app:layout_constraintVertical_bias="0.396"
        app:srcCompat="@drawable/temel_gda" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="86dp"
        android:layout_height="24dp"
        android:text="Temel Gıda"
        app:layout_constraintBottom_toTopOf="@+id/temel.gida"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.357"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/ekmek"
        app:layout_constraintVertical_bias="0.791" />

</androidx.constraintlayout.widget.ConstraintLayout>